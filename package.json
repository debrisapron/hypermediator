{
  "name": "hypermediator",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "test": "TEST=true node ./test/test.js",
    "serve": "NODE_ENV=development nodemon -e js,jsx ./scripts/serveDev.js",
    "livereload": "livereload .app-ready -p 8082",
    "dev": "npm-run-all --parallel serve livereload",
    "db:dev:drop": "psql -c 'DROP DATABASE hm_dev;'",
    "db:dev:create": "psql -c 'CREATE DATABASE hm_dev;'",
    "db:dev:migrate": "knex migrate:latest --env development",
    "db:dev:seed": "knex seed:run --env development",
    "db:dev:reset": "npm-run-all --sequential db:dev:drop db:dev:create db:dev:migrate db:dev:seed",
    "db:start": "sudo service postgresql start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/debrisapron/hypermediator.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/debrisapron/hypermediator/issues"
  },
  "homepage": "https://github.com/debrisapron/hypermediator#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "camelize": "^1.0.0",
    "express": "^4.14.0",
    "express-react-views": "^0.10.2",
    "express-session": "^1.14.2",
    "knex": "^0.12.6",
    "knexnest": "^0.4.0",
    "lodash": "^4.17.4",
    "lru-cache": "^4.0.2",
    "passwordless": "^1.1.2",
    "pg": "^6.1.2",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "shortid": "^2.2.6",
    "showdown": "^1.6.0"
  },
  "devDependencies": {
    "connect-livereload": "^0.6.0",
    "jsdom": "^9.9.1",
    "livereload": "^0.6.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.0",
    "passwordless-memorystore": "0.0.1",
    "tape": "^4.6.3",
    "touch": "^1.0.0"
  }
}
