{
  "name": "hypermediator",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "test": "TEST=true node ./app/index",
    "serve": "NODE_ENV=development nodemon -e js,jsx ./scripts/serveDev.js",
    "livereload": "livereload .app-ready -p 8082",
    "dev": "npm-run-all --parallel serve livereload",
    "db:clear": "rm dev.sqlite3",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "db:reset": "npm-run-all --sequential db:clear db:migrate db:seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/debrisapron/hypermediator.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/debrisapron/hypermediator/issues"
  },
  "homepage": "https://github.com/debrisapron/hypermediator#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "express-react-views": "^0.10.2",
    "express-session": "^1.14.2",
    "knex": "^0.12.6",
    "lodash": "^4.17.4",
    "passwordless": "^1.1.2",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "shortid": "^2.2.6",
    "sqlite3": "^3.1.8"
  },
  "devDependencies": {
    "connect-livereload": "^0.6.0",
    "jsdom": "^9.9.1",
    "livereload": "^0.6.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.0",
    "passwordless-memorystore": "0.0.1",
    "tape": "^4.6.3",
    "touch": "^1.0.0"
  }
}
